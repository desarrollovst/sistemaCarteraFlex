			//variables que especifican el indice del elemento 
			//seleccionado al realizar la modificacion del registro
			public var accionOF:int;
			public var accionLC:int;
			public var accionDisp:int;
			public var indOF:int;
			public var indLC:int;
			public var indDisp:int;
