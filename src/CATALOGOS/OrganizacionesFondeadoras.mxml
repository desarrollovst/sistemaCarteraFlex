			public function mostrarFormOF(tipo:int):void{
				//tipo = 1 NUEVO REGISTRO
				//tipo = 2 MODIFICA REGISTRO
				var comMttoOF:MttoOrgFond = new MttoOrgFond();
				comMttoOF = MttoOrgFond(PopUpManager.createPopUp(this,MttoOrgFond,true)); 
				if(edicion == true){
					switch(tipo){
						case 1: 
							comMttoOF.addEventListener(Event.REMOVED_FROM_STAGE, actualizarOF);
							comMttoOF.init(tipo);
							PopUpManager.centerPopUp(comMttoOF);
							break;
						case 2: 
							if (dgOF.selectedIndex >= 0){
								accionOF = tipo;
								var codOF:String = dgOF.selectedItem.CODIGO;
								var orgFond:String = dgOF.selectedItem.NOMBRE;
								var indice:int = dgOF.selectedIndex;
								iniVar();
								this.indOF = indice;
								this.vScroll = dgOF.verticalScrollPosition;
								comMttoOF.addEventListener(Event.REMOVED_FROM_STAGE, actualizarOF);
								comMttoOF.init(tipo, codOF);	
								PopUpManager.centerPopUp(comMttoOF);	
							}
							else
								Alert.show("Debe seleccionar el registro de la Organización Fondeadora",tituloOF,4,null,null,global.iAlert); 
							break;
					}
				}
			}
			
			public function permisos():void{
				var mod_id:String = Application.application._Current_Mod_Id;
				var perfil_id:String = Application.application.cadPerfil;
				var Params:Array;
				var cont:int;
				
				wsMS = du.initWs(wsMS);
				du.sCursor();
					
				du.ejecutaWsMethod(wsMS,function(evt:ResultEvent):void {											
					dPermisos = new XML(evt.result.toString());
						
					cont = dPermisos.elements().length();
					
					for(var i:int = 0; i < cont; i++){
						switch(dPermisos.Table[i].OPCION.toString()){
							case "C":
								consulta = true;
								break;	
							case "B":
								borrado = true;
								break;
							case "I":
								alta = true;
								break;
							case "E":
								edicion = true;
								break;				
						}
					}
					btnAgregar.enabled = alta;
					du.rCursor();
					du.closeWs(wsMS);					
				});
				Params = new Array();
				Params[0] = perfil_id;
				Params[1] = mod_id;
				wsMS.ESLoginMethods.send(14,Params);	//Método que indica las opciones disponibles para el usuario	
			}
			
			public function seleccionarDisp():void{
				if (dgDisp.selectedIndex >= 0){
					activarContModDisp(true);
				}
				else{
					activarContModDisp(false);
				}
			}
			
			public function seleccionarLC():void{
				dgDisp.dataProvider = null;
				activarContModDisp(false);
				if (dgLC.selectedIndex >= 0){
					activarContModLC(true);
					activarContDisp(true);
				}
				else{
					activarContModLC(false);
					activarContDisp(false);
				}
			}
			
			public function seleccionarOF():void{
				dgLC.dataProvider = null;
				dgDisp.dataProvider = null;
				activarContModLC(false);
				if (dgOF.selectedIndex >= 0){
					activarContOF(true);
					activarContLC(true);
				}
				else{
					activarContOF(false);
					activarContLC(false);
					activarContDisp(false);
				}
			}
			
			private function setAccionDisposicion(event:ResultEvent):void{
				wsMS.disconnect();
				du.rCursor();
				Application.application.desbloquear();
				wsMS.removeEventListener(ResultEvent.RESULT, setAccionDisposicion);
				var res:String = new String(event.result.toString());
				if (res.substr(0,1) == "1")
					buscarListaDisp(1);
				else
					Alert.show(res.substr(2,res.length-1),tituloDisp,4,null,null,global.iAlert);
			}
			
			private function setAccionLineaCred(event:ResultEvent):void{
				wsMS.disconnect();
				du.rCursor();
				Application.application.desbloquear();
				wsMS.removeEventListener(ResultEvent.RESULT, setAccionLineaCred);
				var res:String = new String(event.result.toString());
				if (res.substr(0,1) == "1")
					buscarListaLC(1);
				else
					Alert.show(res.substr(2,res.length-1), tituloLC,4,null,null,global.iAlert);
			}

			private function setAccionOrgFond(event:ResultEvent):void{
				wsMS.disconnect();
				du.rCursor();
				Application.application.desbloquear();
				wsMS.removeEventListener(ResultEvent.RESULT, setAccionOrgFond);
				var res:String = new String(event.result);
				if (res.substr(0,1) == "1")
					buscarOF(1);
				else
					Alert.show(res.substr(2,res.length-1), tituloOF,4,null,null,global.iAlert);
			}
		]]>
	</mx:Script>
