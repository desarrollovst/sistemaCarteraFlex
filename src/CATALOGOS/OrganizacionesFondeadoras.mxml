			//variables que indican los permisos disponibles para el usuario
			public var alta:Boolean = new Boolean();
			public var borrado:Boolean = new Boolean();
			public var consulta:Boolean = new Boolean();
			public var edicion:Boolean = new Boolean();
			
			public function activarContDisp(band:Boolean):void{
				if(alta == true)
					btnAgregarDisp.enabled = band;
				btnDatosDisp.enabled = band;
			}
			
			public function activarContLC(band:Boolean):void{
				if(alta == true)
					btnAgregarLC.enabled = band;
				btnDatosLC.enabled = band;
			}
			
			public function activarContModDisp(band:Boolean):void{
				if(borrado == true)
					btnEliminarDisp.enabled = band;
				if(edicion == true)	
					btnEditarDisp.enabled = band;
			}
			
			public function activarContModLC(band:Boolean):void{
				if(borrado == true)
					btnEliminarLC.enabled = band;
				if(edicion == true)	
					btnEditarLC.enabled = band;
			}
			
			public function activarContOF(band:Boolean):void{
				if(edicion == true)
					btnEditar.enabled = band;
				if(borrado == true)
					btnEliminar.enabled = band;
			}
			
			public function actualizarDisp(event:Event):void{
				buscarListaDisp(1);
			}
			
			public function actualizarLC(event:Event):void{
				buscarListaLC(1);
			}
			
			public function actualizarOF(event:Event):void{
				buscarOF(1);
			}
			
			public function buscarListaDisp(tipo:int):void{
				var wsCat:WebService = new WebService;
				var params:Array = new Array;
				var codOF:String = dgOF.selectedItem.CODIGO;
				var codLC:String = dgLC.selectedItem.CODIGO;
				
				dgDisp.dataProvider = null;
				du.initWsCat(wsCat);
				du.sCursor();
			 	if (tipo == 1)
					Application.application.bloquear();
					
				du.ejecutaWsMethod(wsCat,function(evt:ResultEvent):void {											
					_xmlDisp = new XML(evt.result);
				
					du.rCursor();
					du.closeWs(wsCat);
					Application.application.desbloquear();
					
					if (_xmlDisp.elements().length() > 0){
						dgDisp.dataProvider = _xmlDisp.Table;
						
						if(accionDisp == 2){
							dgDisp.validateNow();
							dgDisp.verticalScrollPosition = vScroll;
							dgDisp.selectedIndex = indDisp;
							this.accionDisp = 0;
						}
					}
					seleccionarDisp();				
				});
				params[0] = codOF;
				params[1] = codLC;
				wsCat.getListado.send(22, params);
			}
			
			public function buscarListaLC(tipo:int):void{
				var wsCat:WebService = new WebService;
				var params:Array = new Array;
				var codOF:String = dgOF.selectedItem.CODIGO;
				
				dgLC.dataProvider = null;
				du.initWsCat(wsCat);
				du.sCursor();
			 	if (tipo == 1)
					Application.application.bloquear();
				
				du.ejecutaWsMethod(wsCat,function(evt:ResultEvent):void {											
					_xmlLC = new XML(evt.result);
				
					du.rCursor();
					du.closeWs(wsCat);
					Application.application.desbloquear();
				
					if (_xmlLC.elements().length() > 0){
						dgLC.dataProvider = _xmlLC.Table;
						
						if(accionLC == 2){
							dgLC.validateNow();
							dgLC.verticalScrollPosition = vScroll;
							dgLC.selectedIndex = indLC;
							this.accionLC = 0;
						}
					}
					seleccionarLC();					
				});
				params[0] = codOF;
				wsCat.getListado.send(21, params);	
			}
			
			public function buscarOF(tipo:int):void{
				var wsCat:WebService = new WebService;
				var params:Array = new Array;
				
				if (txtCodOF.text != "" || txtOF.text != ""){
					btnBuscar.setFocus();
					dgOF.dataProvider = null;
					
					du.initWsCat(wsCat);
					du.sCursor();
					if(tipo == 1)
						Application.application.bloquear();
						
					du.ejecutaWsMethod(wsCat,function(evt:ResultEvent):void {											
						_xmlOF = new XML(evt.result);
						
						du.rCursor();
						du.closeWs(wsCat);
						Application.application.desbloquear();
						
						if (_xmlOF.elements().length() > 0){
							dgOF.dataProvider = _xmlOF.Table;
							
							if(accionOF == 2){
								dgOF.validateNow();
								dgOF.verticalScrollPosition = vScroll;
								dgOF.selectedIndex = indOF;
								this.accionOF = 0;
							}
						}
						seleccionarOF();
					});
					params[0] = txtCodOF.text;
					params[1] = txtOF.text;
					wsCat.getListado.send(20, params);	
				}	
			}

			public function eliminarDisp():void{
				Alert.show("¿Desea eliminar la Disposición seleccionada?", tituloDisp, Alert.YES|Alert.NO,null, manejadorEliminarDisp,global.iQuest);
			}
			
			public function eliminarLC():void{
				Alert.show("¿Desea eliminar la Línea de Crédito seleccionada?", tituloLC, Alert.YES|Alert.NO,null, manejadorEliminarLC,global.iQuest);
			}
			
			public function eliminarOF():void{
				Alert.show("¿Desea eliminar la Organización Fondeadora seleccionada?", tituloOF, Alert.YES|Alert.NO,null, manejadorEliminarOF,global.iQuest);
			}
			
			public function iniVar():void{
				this.indOF = -1;
				this.indLC = -1;
				this.indDisp = -1;
				this.vScroll = -1;
			}
			
			public function initApp():void{
				global = new Globales();
				du = new Utils();
				wsMS = new WebService();
				tituloOF = "Mantenimiento de Org Fondeadora";
				tituloLC = "Mantenimiento de Línea de Crédito";
				tituloDisp = "Mantenimiento de Disposición";
				alta = false;
				borrado = false;
				consulta = false;
				edicion = false;
				permisos();
			}
			
			public function initConexion():void{				
				wsMS = new WebService();			
				wsMS.wsdl = Application.application.wsStr.wsdlServ.toString();
				wsMS.loadWSDL();
			}
			
			public function manejadorEliminarDisp(e:CloseEvent):void{
				if(e.detail==Alert.YES){
					var codOF:String = dgOF.selectedItem.CODIGO;
					var codLC:String = dgLC.selectedItem.CODIGO;  
					var codDisp:String = dgDisp.selectedItem.CODIGO;
					var arrEnt:Array = new Array;
					var arrMun:Array = new Array;
					arrEnt[0] = "000";
					arrMun[0] = "";
			    	initConexion();
					du.sCursor();
					Application.application.bloquear();
					wsMS.addEventListener(ResultEvent.RESULT, setAccionDisposicion);
					wsMS.setAccionDisposicion.send(3, codOF, codLC, codDisp, "", "", "", 0, 0, "", "", 0, 0, arrEnt, arrMun,
												   Application.application.U_ID, 0, "", "", "");
			    } 
			}
			
			public function manejadorEliminarLC(e:CloseEvent):void{
				if(e.detail==Alert.YES){
					var codOF:String = dgOF.selectedItem.CODIGO;
					var codLC:String = dgLC.selectedItem.CODIGO;  
			    	initConexion();
			    	du.sCursor();
					Application.application.bloquear();
					wsMS.addEventListener(ResultEvent.RESULT, setAccionLineaCred);
					wsMS.setAccionLineaCred.send(3, codOF, codLC, "", "", "", 0, 0, 0, 0, "", "", 0, 0, "", "", "",
								 	     		 Application.application.U_ID);
			    } 
			}
			
			public function manejadorEliminarOF(e:CloseEvent):void{
				if(e.detail==Alert.YES){
					var codOF:String = dgOF.selectedItem.CODIGO;
			    	initConexion();
			    	du.sCursor();
					Application.application.bloquear();
					wsMS.addEventListener(ResultEvent.RESULT, setAccionOrgFond);
					wsMS.setAccionOrgFond.send(3, codOF, "", "", "", "", "", "", "", "", 
								 	   		   "", "", Application.application.U_ID);
			    } 
			}
			
			public function mostrarFormDisp(tipo:int):void{
				//tipo = 1 NUEVO REGISTRO
				//tipo = 2 MODIFICA REGISTRO
				if (edicion == true){
					var codOF:String = dgOF.selectedItem.CODIGO;
					var orgFond:String = dgOF.selectedItem.NOMBRE;
					var codLC:String = dgLC.selectedItem.CODIGO;
					var lineaCred:String = dgLC.selectedItem.DESCRIPCION;
					var comMttoDisp:MttoDisposicion = new MttoDisposicion();
					switch(tipo){
						case 1: 
							comMttoDisp = MttoDisposicion(PopUpManager.createPopUp(this,MttoDisposicion,true)); 
							comMttoDisp.addEventListener(Event.REMOVED_FROM_STAGE, actualizarDisp);
							comMttoDisp.init(tipo, codOF, orgFond, codLC, lineaCred);
							PopUpManager.centerPopUp(comMttoDisp);
							break;
						case 2: 
							if (dgDisp.selectedIndex >= 0){
								accionDisp = tipo;
								comMttoDisp = MttoDisposicion(PopUpManager.createPopUp(this,MttoDisposicion,true)); 
								var codDisp:String = dgDisp.selectedItem.CODIGO;
								var indice:int = dgDisp.selectedIndex;
								iniVar();
								this.indDisp = indice;
								this.vScroll = dgDisp.verticalScrollPosition;
								comMttoDisp.addEventListener(Event.REMOVED_FROM_STAGE, actualizarDisp);
								comMttoDisp.init(tipo, codOF, orgFond, codLC, lineaCred, codDisp);
								PopUpManager.centerPopUp(comMttoDisp);
							}
							else
								Alert.show("Debe seleccionar el registro de Disposición",tituloDisp,4,null,null,global.iAlert);
						break;
					}
				}
			}
			
			public function mostrarFormLC(tipo:int):void{
				//tipo = 1 NUEVO REGISTRO
				//tipo = 2 MODIFICA REGISTRO
				if (edicion == true){
					var codOF:String = dgOF.selectedItem.CODIGO;
					var orgFond:String = dgOF.selectedItem.NOMBRE;
					var comMttoLC:MttoLineaCred = new MttoLineaCred();
					switch(tipo){
						case 1: 
							comMttoLC = MttoLineaCred(PopUpManager.createPopUp(this,MttoLineaCred,true)); 
