			public function buscarEntFed():void{
				//busqueda utilizando los campos de codigo y nombre de entidad
				if(txtCodEntFed.text != "" && txtEntFed.text != "")
					dpEntFed = _xmlEntFed.Table.(CODIGO.toString().substr(0, txtCodEntFed.length).toLowerCase() == txtCodEntFed.text.toLowerCase() && NOMBRE.toString().substr(0, txtEntFed.length).toLowerCase() == txtEntFed.text.toLowerCase());
				//busqueda utilizando el campo de codigo de la entidad federativa
				else if	(txtCodEntFed.text != "" && txtEntFed.text == "")
					dpEntFed = _xmlEntFed.Table.(CODIGO.toString().substr(0, txtCodEntFed.length).toLowerCase() == txtCodEntFed.text.toLowerCase());
				//busqueda utilizando el campo de nombre de la entidad federativa
				else if (txtCodEntFed.text == "" && txtEntFed.text != "")
					dpEntFed = _xmlEntFed.Table.(NOMBRE.toString().substr(0, txtEntFed.length).toLowerCase() == txtEntFed.text.toLowerCase());
				else
					dpEntFed = _xmlEntFed.elements();
				seleccionaEntFed();
			}
			
			public function buscarListaCol(tipo:int):void{
				var wsCat:WebService = new WebService;
				var params:Array = new Array;
				var entFed:String = dgEntFed.selectedItem.CODIGO;
				var munic:String = dgMunic.selectedItem.codigo;
				var localidad:String = dgLocalidad.selectedItem.codigo;
				
				dgColonia.dataProvider = null;
				du.initWsCat(wsCat);
				du.sCursor();
				
			 	if (tipo == 1)
					global.bloquear();
				du.ejecutaWsMethod(wsCat,function(evt:ResultEvent):void {											
					_xmlColonia = new XML(evt.result.toString());
					
					if(_xmlColonia.elements().length() > 0){
						formateaColonia();
						dgColonia.dataProvider = colObj;
					}
					if(accionCol == 2){
						dgColonia.validateNow();
						dgColonia.verticalScrollPosition = vScroll;
						dgColonia.selectedIndex = indice;
						accionCol = 0;
					}
					seleccionaCol();
					du.rCursor();
					du.closeWs(wsCat);
					global.desbloquear();
				});
				params[0] = entFed;
				params[1] = munic;
				params[2] = localidad;
				params[3] = "";
				wsCat.getCatalogoGral.send(5, params);
			}

