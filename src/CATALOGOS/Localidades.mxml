			public function initConexion():void{				
				wsMS = new WebService();			
				wsMS.wsdl = Application.application.wsStr.wsdlServ.toString();
				wsMS.loadWSDL();
			}		
			
			public function manejadorEliminarCol(e:CloseEvent):void{
				if(e.detail==Alert.YES){
					var cdgEntFed:String = dgEntFed.selectedItem.CODIGO;
					var cdgMunic:String = dgMunic.selectedItem.codigo;
					var cdgLoc:String = dgLocalidad.selectedItem.codigo;
					var cdgCol:String = dgColonia.selectedItem.codigo; 
					initConexion();
					du.sCursor();
			 		du.ejecutaWsMethod(wsMS,function(evt:ResultEvent):void {											
						var res:String = evt.result.toString();
					
						du.rCursor();
						du.closeWs(wsMS);
						global.desbloquear();
						
						if (res.substr(0,1) == "1")
							buscarListaCol(1);
						else
							Alert.show(res.substr(2,res.length-1), "Mantenimiento de Colonias",4,null,null,global.iAlert);
					});
					wsMS.setAccionCol.send(3, cdgEntFed, cdgMunic, cdgLoc, cdgCol);
				} 
			}
			
			//Muestra el formulario de edicion de localidad
			public function mostrarFormLoc(tipo:int):void{
				//tipo = 2 MODIFICA REGISTRO
				var cdgEntFed:String;
				var entFed:String;
				var cdgMunic:String;
				var munic:String;
				if(edicion == true){
					var comMttoLoc:MttoLocalidad = new MttoLocalidad();
					cdgEntFed = dgEntFed.selectedItem.CODIGO;
					entFed = dgEntFed.selectedItem.NOMBRE;
					cdgMunic = dgMunic.selectedItem.codigo;
					munic = dgMunic.selectedItem.nombre;
					comMttoLoc = MttoLocalidad(PopUpManager.createPopUp(this,MttoLocalidad,true));  
					comMttoLoc.addEventListener(Event.REMOVED_FROM_STAGE, actualizarLoc);			
					comMttoLoc.init(cdgEntFed, entFed, cdgMunic, munic);
					switch(tipo){
						case 1:
								comMttoLoc.registraInfoLoc();
								PopUpManager.centerPopUp(comMttoLoc);
							break;
						case 2: 
							if (dgLocalidad.selectedIndex >= 0){
								accionLoc = tipo;
								var cdgLoc:String = dgLocalidad.selectedItem.codigo;
								iniVar();
								this.indice = dgLocalidad.selectedIndex;
								this.vScroll = dgLocalidad.verticalScrollPosition;
								comMttoLoc.cargaInfoLoc(cdgLoc);
								PopUpManager.centerPopUp(comMttoLoc);
							}
							else
								Alert.show("Debe seleccionar el registro de la Localidad","Mantenimiento de Localidades",4,null,null,global.iAlert); 
							break;
					}
				}
			}	
			
			//Muestra el formulario de edicion de colonia 
			public function mostrarFormCol(tipo:int):void{
				//tipo = 2 MODIFICA REGISTRO
				var cdgEntFed:String;
				var entFed:String;
				var cdgMunic:String;
				var munic:String;
				var cdgLoc:String;
				var localidad:String;
				if(edicion == true){
					var comMttoCol:MttoColonia = new MttoColonia();
					cdgEntFed = dgEntFed.selectedItem.CODIGO;
					entFed = dgEntFed.selectedItem.NOMBRE;
					cdgMunic = dgMunic.selectedItem.codigo;
					munic = dgMunic.selectedItem.nombre;
					cdgLoc = dgLocalidad.selectedItem.codigo;
					localidad = dgLocalidad.selectedItem.nombre;
					
					switch(tipo){
						case 1:
								comMttoCol = MttoColonia(PopUpManager.createPopUp(this,MttoColonia,true));  
								comMttoCol.addEventListener(Event.REMOVED_FROM_STAGE, actualizarCol);			
								comMttoCol.init(cdgEntFed, entFed, cdgMunic, munic, cdgLoc, localidad);
								comMttoCol.registraInfoCol();
								PopUpManager.centerPopUp(comMttoCol);
							break;
						case 2: 
							if (dgColonia.selectedIndex >= 0){
								accionCol = tipo;
								var cdgCol:String = dgColonia.selectedItem.codigo;
								iniVar();
								this.indice = dgColonia.selectedIndex;
								this.vScroll = dgColonia.verticalScrollPosition;
								comMttoCol = MttoColonia(PopUpManager.createPopUp(this,MttoColonia,true));  
								comMttoCol.addEventListener(Event.REMOVED_FROM_STAGE, actualizarCol);			
								comMttoCol.init(cdgEntFed, entFed, cdgMunic, munic, cdgLoc, localidad);
								comMttoCol.cargaInfoCol(cdgCol);
								PopUpManager.centerPopUp(comMttoCol);
							}
							else
								Alert.show("Debe seleccionar el registro de la Colonia","Mantenimiento de Colonias",4,null,null,global.iAlert); 
							break;
					}
				}
			}	
			
			public function permisos():void{
				var mod_id:String = Application.application._Current_Mod_Id;
				var perfil_id:String = Application.application.cadPerfil;
				var Params:Array;
				var cont:int;
				
				ws = du.initWs(ws);
				du.sCursor();
					
				du.ejecutaWsMethod(ws,function(evt:ResultEvent):void {											
					dPermisos = new XML(evt.result.toString());
						
					cont = dPermisos.elements().length();
					
					for(var i:int = 0; i < cont; i++){
						switch(dPermisos.Table[i].OPCION.toString()){
							case "C":
								consulta = true;
								if (permiso.permisosLocalidades(Application.application.PERFIL_ID)){
									borrado = true;
									alta = true;
									edicion = true;
								}
								break;	
							/*case "B":
								borrado = true;
								break;
							case "I":
								alta = true;
								break;
							case "E":
								edicion = true;
								break;*/
						}
					}
					du.rCursor();
					du.closeWs(ws);					
				});
				Params = new Array();
				Params[0] = perfil_id;
				Params[1] = mod_id;
				ws.ESLoginMethods.send(14,Params);	//MÃ©todo que indica las opciones disponibles para el usuario	
			}
			
			public function seleccionaCol():void{
				if (dgColonia.selectedIndex >= 0){
					activarContModCol(true);
				}
				else{
					activarContModCol(false);
				}
			}
			
			public function seleccionaEntFed():void{
				dgMunic.dataProvider = null;
				dgLocalidad.dataProvider = null;
				dgColonia.dataProvider = null;
				activarContMunic(false);
				activarContLoc(false);
				activarContModLoc(false);
				activarContCol(false);
				activarContModCol(false);
				if (dgEntFed.selectedIndex >= 0){
					activarContMunic(true);
				}
				else{
					activarContMunic(false);
				}
			}
			
			public function seleccionaLoc():void{
				dgColonia.dataProvider = null;
				activarContCol(false);
				activarContModCol(false);
				if (dgLocalidad.selectedIndex >= 0){
					activarContCol(true);
					activarContModLoc(true);
				}
				else{
					activarContCol(false);
					activarContModCol(false);
				}
			}
			
			public function seleccionaMunic():void{
				dgLocalidad.dataProvider = null;
				dgColonia.dataProvider = null;
				activarContLoc(false);
				activarContModLoc(false);
				activarContCol(false);
				activarContModCol(false);
				if (dgMunic.selectedIndex >= 0){
					activarContLoc(true);
				}
				else{
					activarContLoc(false);
				}
			}
			
		]]>
