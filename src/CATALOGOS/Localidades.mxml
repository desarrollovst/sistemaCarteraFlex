			public function cargaEntFed():void{
				var wsCat:WebService = new WebService;
				var params:Array = new Array;
				
				du.initWsCat(wsCat);
				du.sCursor();
					
				du.ejecutaWsMethod(wsCat,function(evt:ResultEvent):void {											
					_xmlEntFed = new XML(evt.result.toString());
					dpEntFed = _xmlEntFed.elements(); 
					
					du.rCursor();
					du.closeWs(wsCat);
					
					if (_xmlEntFed.elements().length() > 0){
						dgEntFed.dataProvider = dpEntFed;
					} 	
				});
				wsCat.getCatalogoGral.send(2);
			}

			public function eliminarColonia():void{
				Alert.show("Â¿Desea eliminar la colonia seleccionada?", "Mantenimiento de Colonias", Alert.YES|Alert.NO,null, manejadorEliminarCol,global.iQuest);
			}
			
			private function formateaEntFed():void{
				var cont:int = _xmlEntFed.elements().length();
				var oItem:Object;
				var item:Array = new Array;
				
				entFedObj.filterFunction = null;
				entFedObj.refresh();
				entFedObj.removeAll();
				entFedObj.refresh();
				
				for (var i:int = 0; i < cont; i++){
					oItem = new Object();
					oItem.nombre = _xmlEntFed.Table[i].NOMBRE;
					oItem.codigo = _xmlEntFed.Table[i].CODIGO;
					oItem.abreviatura = _xmlEntFed.Table[i].ABREVIATURA;
					item.push(oItem);
				}
				entFedObj = new ArrayCollection(item);
			}
			
