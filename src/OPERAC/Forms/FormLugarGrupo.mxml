			public function initConexion():void{				
				wsMS = new WebService();			
				wsMS.wsdl = Application.application.wsStr.wsdlServ.toString();
				wsMS.loadWSDL();
			}
			
						
			public function init(info:DatosGrupo, tipoAccion:int):void{
				var wsCat:WebService = new WebService;
				var params:Array = new Array;
				global = new Globales();
				du = new Utils();
				
				du.initWsCat(wsCat);
				du.sCursor();
					
				_xmlEntidad = null;
				
				du.ejecutaWsMethod(wsCat,function(evt:ResultEvent):void {											
					_xmlEntidad = new XML(evt.result.toString());
					
					if(_xmlEntidad.elements().length() > 0){
						formateaEntidad();
						dgEntidad.dataProvider = entObj;
					}
					
					du.rCursor();
					du.closeWs(wsCat);
					
					if (info != null){
						txtCalle.text = info.calle;
						txtCodPostal.text = info.lugarCodPostal;
						txtEntFed.text = info.lugarEntFed;
						txtMunicipio.text = info.lugarMun;
						txtLocalidad.text = info.lugarLocalidad;
						txtColonia.text = info.lugarColonia;
						
						datos.cdgLugarEntFed = info.cdgLugarEntFed;		
						datos.cdgLugarMun = info.cdgLugarMun;
						datos.cdgLugarLoc = info.cdgLugarLoc;
						datos.cdgLugarCol = info.cdgLugarCol;							
						
						obtieneMunicipio();
						obtieneLocalidad();
						obtieneColonia();
						
						txtEntFed.addEventListener(KeyboardEvent.KEY_DOWN, presEntidad);
					}	
				});
				wsCat.getCatalogoGral.send(2);
			}
			
