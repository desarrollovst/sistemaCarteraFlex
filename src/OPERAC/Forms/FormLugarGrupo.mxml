			public function enviarLugarGrupo(formData:DatosGrupo):void{
				var invalidArray:Array = Validator.validateAll([calleV, entFedV, municipioV, localidadV, coloniaV]);
				if(invalidArray.length == 0){	
					formData.calle = StringUtil.trim(txtCalle.text);
					formData.lugarCodPostal = txtCodPostal.text;
					formData.lugarEntFed = txtEntFed.text;
					formData.lugarMun = txtMunicipio.text;
					formData.cdgLugarMun = datos.cdgLugarMun;
					formData.lugarLocalidad = txtLocalidad.text;
					formData.cdgLugarLoc = datos.cdgLugarLoc;
					formData.lugarColonia = txtColonia.text;
					formData.cdgLugarCol = datos.cdgLugarCol;
					formData.guardaLugar = true;
				}
				else
					formData.guardaLugar = false;
				
				var event:EventGrupo = new EventGrupo("enviarLugarGrupo", formData);
				dispatchEvent(event);
			}
			
			private function filtraCaptura(item:Object):Boolean{
				var isMatch:Boolean = false
				if(texto.toLowerCase().search(item.nombre.toLowerCase()) != -1)
					isMatch = true 
				return isMatch; 
			}
			
			public function filtraColonia(item:Object):Boolean{
				var isMatch:Boolean = false
				if(item.nombre.toString().substr(0, texto.length).toLowerCase() == texto.toLowerCase())
