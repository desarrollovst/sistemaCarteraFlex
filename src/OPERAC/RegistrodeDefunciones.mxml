	<mx:Script>
		<![CDATA[
			private function eliminarDefun():void{
				if(dgDefun.selectedIndex >= 0){
					if(dgDefun.selectedItem.CODIGO != "")
						Alert.show("Â¿Desea eliminar la defunciÃ³n seleccionada?", titulo, Alert.YES|Alert.NO,null, manejadorEliminarDefun,global.iQuest);
					else
						Alert.show("Seleccione la defunciÃ³n a eliminar.", titulo, 4,null,null,global.iAlert);
