				if(e.detail==Alert.YES){
					infoDef = new DatosDefuncion();
					infoDef.codigo = dgDefun.selectedItem.CODIGO;
					infoDef.nombre1 = "";
					infoDef.nombre2 = "";
					infoDef.primape = "";
					infoDef.segape = "";
					infoDef.sexo = "";
					infoDef.nacimiento = "";
					infoDef.defuncion = "";
					infoDef.edad = 0;
					infoDef.causamuerte = "";
					infoDef.cdgtdef = "";
					infoDef.saldodeudor = 0;
					infoDef.docorig = "";
					infoDef.pagado = "";
